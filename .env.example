# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/uncensored_ai"

# NextAuth Configuration
NEXTAUTH_SECRET="generate-with-openssl-rand-base64-32"
NEXTAUTH_URL="http://localhost:3000"

# AI Provider Configuration
# Choose provider: 'openai' or 'venice'
AI_PROVIDER="venice"

# OpenAI API (used when AI_PROVIDER=openai)
OPENAI_API_KEY="sk-your-openai-api-key-here"

# Venice.ai API (used when AI_PROVIDER=venice)
VENICE_API_KEY="your-venice-api-key-here"
VENICE_MODEL="llama-3.3-70b"

# Stripe Configuration
STRIPE_SECRET_KEY="sk_test_your_stripe_secret_key"
STRIPE_WEBHOOK_SECRET="whsec_your_webhook_secret"
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="pk_test_your_publishable_key"

# Redis (Optional)
REDIS_URL="redis://localhost:6379"

# Admin Account
ADMIN_EMAIL="admin@example.com"
ADMIN_PASSWORD="change-this-to-a-secure-password"
